{
  "name": "mediasoup-client",
  "version": "2.0.18",
  "description": "mediasoup client side JavaScript SDK",
  "author": "Iñaki Baz Castillo <ibc@aliax.net> (https://inakibaz.me)",
  "contributors": [
    "José Luis Millán <jmillan@aliax.net> (https://github.com/jmillan)"
  ],
  "homepage": "https://mediasoup.org",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/versatica/mediasoup-client.git"
  },
  "main": "build/index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "precommit":
      "lint-staged && npm run build && (git add build || echo 'could not add build files')",
    "build": "babel lib -d ./build --ignore '**/*.test.js'",
    "build:clean": "rm -rf build/"
  },
  "dependencies": {
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "bowser": "^1.9.3",
    "debug": "^3.1.0",
    "events": "^2.0.0",
    "random-number": "0.0.9",
    "sdp-transform": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^4.19.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json}": [
        "prettier-eslint --single-quote --trailing-comma es5 --write",
        "git add"
      ],
      "*.js": ["eslint --fix", "git add"]
    },
    "concurrent": false
  }
}
